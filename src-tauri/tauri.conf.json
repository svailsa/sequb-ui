{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Sequb",
        "width": 1400,
        "height": 900,
        "minWidth": 1024,
        "minHeight": 768
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "category": "Productivity",
    "copyright": "",
    "externalBin": [
      "binaries/sequb-server"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "identifier": "com.sequb.ui",
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "productName": "Sequb",
  "identifier": "com.sequb.ui",
  "plugins": {
    "shell": {
      "open": true,
      "scope": [
        {
          "name": "sequb-server",
          "sidecar": true,
          "args": []
        }
      ]
    }
  },
  "mainBinaryName": "sequb-ui"
}