{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": {
        "default-src": ["'self'"],
        "script-src": ["'self'"],
        "style-src": ["'self'", "'unsafe-inline'"],
        "img-src": ["'self'", "data:", "https:"],
        "connect-src": ["'self'", "http://localhost:*", "http://127.0.0.1:*"],
        "font-src": ["'self'", "data:"],
        "media-src": ["'self'"],
        "object-src": ["'none'"],
        "base-uri": ["'none'"],
        "form-action": ["'self'"],
        "frame-ancestors": ["'none'"],
        "block-all-mixed-content": true,
        "upgrade-insecure-requests": true
      },
      "dangerousDisableAssetCspModification": false,
      "freezePrototype": true
    },
    "windows": [
      {
        "title": "Sequb",
        "width": 1400,
        "height": 900,
        "minWidth": 1024,
        "minHeight": 768,
        "resizable": true,
        "fullscreen": false,
        "alwaysOnTop": false,
        "fileDropEnabled": false,
        "decorations": true
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "category": "Productivity",
    "copyright": "",
    "externalBin": [
      "binaries/sequb-server"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "identifier": "com.sequb.ui",
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "productName": "Sequb",
  "identifier": "com.sequb.ui",
  "plugins": {
    "shell": {
      "open": false,
      "scope": [
        {
          "name": "sequb-server",
          "sidecar": true,
          "args": {
            "validator": "\\S+"
          }
        }
      ]
    }
  },
  "mainBinaryName": "sequb-ui"
}